{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 80
      }
    ],
    "consistent-return": "off",
    "func-names": "off",
    "prefer-destructuring": "off"
  }
}
